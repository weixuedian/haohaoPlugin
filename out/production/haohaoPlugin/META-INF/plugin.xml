<idea-plugin>
    <id>haohaoPlugin</id>
    <name>haohaoPlugin</name>
    <version>1.0</version>
    <vendor email="haohaoPlugin@yourcompany.com" url="http://www.haohaoPlugin.com">haohaoPlugin</vendor>

    <description><![CDATA[
        upload api to yapi<br>
      <em>support dubbo api and api upload</em>
    ]]></description>

    <change-notes><![CDATA[
      upload api to yapi<br>
      <em>support dubbo api and api upload</em>
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="173.0"/>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="HaoHaoYapi.HaoHaoYapi" class="com.haohao.tiger.HaoHaoYapi" text="HaoHaoYapi"
                description="HaoHaoYapi">
            <add-to-group group-id="EditorLangPopupMenu" anchor="first"/>
            <keyboard-shortcut keymap="$default" first-keystroke="alt 2"/>
        </action>
    </actions>

</idea-plugin>